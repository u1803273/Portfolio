function Cell(){
  this.filled = false;
  this.start = false;
  this.end = false;
  this.visited = false;
  this.x=0;
  this.y=0;
  this.parentX=0;
  this.parentY=0;

  // Distance from source to this node
  this.distance=0;
  // Cost of the node (determined by the topography)
  this.cost=0;

  // Used for A* search
  this.f=0;
  this.g=0;
  this.h=0;
}
